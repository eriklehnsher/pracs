<odoo>
    <template id="report_lead_specific_fields_template">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="lead">
                <table class="table table-sm">

                    <tr>
                        <th>Sale Team</th>
                        <td t-esc="lead.team_id.name if lead.team_id else ''"/>
                    </tr>
                    <tr>
                        <th>Min Revenue</th>
                        <td t-esc="lead.min_revenue"/>
                    </tr>
                    <tr>
                        <th>Create Month</th>
                        <td t-esc="lead.create_date.strftime('%m') if lead.create_date else ''"/>
                    </tr>
                </table>
            </t>
        </t>
    </template>
</odoo>
