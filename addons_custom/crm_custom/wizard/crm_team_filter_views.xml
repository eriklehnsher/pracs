<odoo>
    <data>
        <record id="view_crm_lead_new_to_form" model="ir.ui.view">
            <field name="name">crm.lead.new.filter.form</field>
            <field name="model">crm.lead.new.filter</field>
            <field name="arch" type="xml">
                <form string="CRM lead Filter">
                    <group>
                        <field name="team_id"/>
                        <field name="month"/>
                    </group>
                    <footer>
                        <button name="action_apply_filter" string="Xuất Dữ Liệu" type="object" class="btn-primary"/>
                        <button string="Hủy" class="btn-secondary" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>
        <record id="action_crm_lead_new_filter" model="ir.actions.act_window">
            <field name="name">Báo Cáo Đánh Giá Chỉ Tiêu</field>
            <field name="res_model">crm.team.filter</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>
        <menuitem id="menu_crm_lead_new_filter" name="Báo Cáo Đánh Giá Chỉ Tiêu" parent="crm.crm_menu_report" action="action_crm_lead_new_filter"/>
    </data>
</odoo>